{
    "name": "pipe-language-support",
    "displayName": "Pipe language support",
    "description": "Basic support for the pipe programming language",
    "icon": "./logo.png",
    "repository": {
        "url": "https://github.com/Ulfasaar/pipe"
    },
    "version": "0.0.1",
    "publisher": "rjweyers",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "pipe",
            "aliases": ["Pipe", "pipe"],
            "extensions": [".[]",".pipe"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pipe",
            "scopeName": "source.pipe",
            "path": "./syntaxes/pipe.tmLanguage.json"
        }]
    }
}